<h1>アカウント情報</h1>

<table>
  <tbody>
    <tr>
      <th>アカウント名</th>
      <td><%= @editor.account.name %></td>
    </tr>
    <tr>
      <th>メールアドレス</th>
      <td><%= @editor.account.email %></td>
    </tr>
    <tr>
      <th>管理者権限</th>
      <td><%= get_authority_status(@editor.account) %></td>
    </tr>
  </tbody>
</table>